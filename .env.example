# Google Calendar Credentials
# Get these from Google Cloud Console
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
# Use OAuth2 Playground to get this
GOOGLE_REFRESH_TOKEN=your_google_refresh_token

# Cal.com Credentials
# Get API key from Cal.com > Settings > API Keys
CAL_API_KEY=your_cal_api_key
# The ID of the specific event type you want to book
CAL_EVENT_TYPE_ID=your_event_type_id
# Your cal.com username (the part after cal.com/)
CAL_USERNAME=your_cal_username

# SMTP Credentials for Email
# For Gmail, use an "App Password"
SMTP_USER=your_smtp_user
SMTP_PASS=your_smtp_pass
# The email address that will receive/send notifications
USER_EMAIL=your_email@example.com
